        var app = new Vue({
            el: '#loginForm',
            data: {
                errorMessage: ''
            },
            methods: {
                validateEmail: function() {
                    var email = this.$el.querySelector('input[name="email"]').value;
                    if (!email.includes('@') || !email.endsWith('@gmail.com')) {
                        this.errorMessage = 'Geçersiz e mail formatı. Lütfen tekrar deneyin.';
                        return false;
                    }
                    return true;
                },
                validatePassword: function() {
                    var password = this.$el.querySelector('input[name="password"]').value;
                    return true;
                },
                onSubmit: function() {
                    if (this.validateEmail() && this.validatePassword()) {
                        this.$el.submit();
                    }
                }
            }
        });
    </script>
